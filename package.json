{
  "name": "tuxies-wiki",
  "type": "module",
  "version": "1.0.0",
  "description": "Tuxie's Wiki",
  "license": "MIT",
  "engines": {
    "node": "^20.6.0 || >=22.0.0"
  },
  "scripts": {
    "dev": "vuepress dev docs",
    "start": "vuepress dev docs",
    "dev-clean": "vuepress dev docs --clean-cache --clean-temp",
    "build": "vuepress build docs --clean-cache --clean-temp",
    "vp-update": "npx vp-update",
    "update-deps": "npx npm-check-updates -u && npm install",
    "deploy": "npm run build && gh-pages -d docs/.vuepress/dist -b gh-pages"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "vue": "^3.5.24",
    "vuepress": "^2.0.0-rc.26",
    "vuepress-theme-plume": "1.0.0-rc.176"
  }
}
